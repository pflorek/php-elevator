{
    "name": "pflorek/elevator",
    "description": "Elevates a map which keys are a materialized path to a tree. Folds a tree to a map by materializing the paths to the leafs.",
    "type": "library",
    "keywords": [
        "map",
        "materialized path",
        "serialize",
        "tree"
    ],
    "time": "2017-03-17",
    "license": "MIT",
    "authors": [
        {
            "name": "Patrick Florek",
            "email": "patrick.florek@gmail.com",
            "homepage": "http://www.patrickflorek.com",
            "role": "Developer"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    },
    "autoload": {
        "psr-4": {
            "PFlorek\\Elevator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PFlorek\\Elevator\\Tests\\": "tests/"
        }
    }
}
